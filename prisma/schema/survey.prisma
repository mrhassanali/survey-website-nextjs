model Survey {
  id           String   @id @default(uuid())
  title        String
  description  String
  reward       Float
  status       SurveyStatus
  completedAt  DateTime?
  user         User     @relation(fields: [userId], references: [id])
  userId       String
  paymentStatus PaymentStatus @default(PENDING)
}

enum SurveyStatus {
  COMPLETED
  DISQUALIFIED
}

enum PaymentStatus {
  PENDING
  PAID
}